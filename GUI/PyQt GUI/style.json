		"defaultSize": [{
			"width": 0,
			"height": "parent"
		}],
		"collapsedSize": [{
			"width": 0,
			"height": "parent"
		}],
		"expandedSize": [{
			"width": 175,
			"height": "parent"
		}],
		"menuTransitionAnimation": [{
			"animationDuration": 250,
			"animationEasingCurve": "Linear"
		}]
	}]
